
public class MathCalculations {

	public static void main(String[] args) {
		Log("5 squared is " + Math.pow(5, 2));
		Log("The absolute value of -9 is " + Math.abs(-9));
		Log("The square root of 16 is " + Math.sqrt(16));
		Log("The minimum of 14 and 33 is " + Math.min(14, 33));
		Log("The maximum of 39 and 21 is " + Math.max(39, 21));
		Log("15.5 rounded to the nearest integer is " + Math.round(15.5));
		
	}

	//Method to make printing easier
	public static void Log(String input) {
		System.out.println(input);
	}
}
